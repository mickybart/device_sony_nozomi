# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
#
# fstab.mount:dynamic

#<src>                                    <mnt_point>       <type>  <mnt_flags and options>                                       <fs_mgr_flags>
/dev/block/platform/msm_sdcc.1/by-num/p12  /system           ext4    ro,barrier=1                                                  wait

/dev/block/platform/msm_sdcc.1/by-num/p10  /idd              ext4    noatime,nosuid,nodev,barrier=0,context=u:object_r:idd_data_file:s0  wait,check

#cache-ext4
#/dev/block/platform/msm_sdcc.1/by-num/p13  /cache            ext4    noatime,nosuid,nodev,barrier=1,data=ordered,nomblk_io_submit,errors=panic  wait,check
#data-ext4
#/dev/block/platform/msm_sdcc.1/by-num/p14  /data             ext4    noatime,nosuid,nodev,barrier=1,data=ordered,nomblk_io_submit,errors=panic  wait,check,formattable,encryptable=footer,length=-16384

#cache-f2fs
#/dev/block/platform/msm_sdcc.1/by-num/p13  /cache            f2fs    noatime,nosuid,nodev,discard,nodiratime,inline_data,inline_xattr,errors=recover     wait,check
#data-f2fs
#/dev/block/platform/msm_sdcc.1/by-num/p14  /data             f2fs    noatime,nosuid,nodev,discard,nodiratime,inline_data,inline_xattr,errors=recover     wait,check,formattable,encryptable=footer,length=-16384

/devices/platform/msm_hsusb_host.0         auto              auto    defaults                                                      voldmanaged=usb:auto

